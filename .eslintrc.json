{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "settings": {
    "react": {
            "version": "detect"
        }
    },
    "extends": [
        "prettier",
        "prettier/@typescript-eslint",
        "prettier/react"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "react",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": ["error", {
            "endOfLine":"auto"
        }]
    }
}
